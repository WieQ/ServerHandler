{
    "name": "ServerHandler Dev",
    "dockerComposeFile": [
        "../docker-compose.yaml"
    ],
    "service": "backend",
    "workspaceFolder": "/app",
    "settings": {
        "terminal.integrated.defaultProfile.linux": "bash"
    },
    "extensions": [
        "ms-python.python",
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint"
    ],
    "postCreateCommand": "pip install --no-cache-dir -r requirements.txt",
    "remoteUser": "root"
}